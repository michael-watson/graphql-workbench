{
  "name": "graphql-workbench",
  "displayName": "GraphQL Workbench",
  "description": "Embed GraphQL schemas and generate operations from natural language",
  "version": "0.1.5",
  "publisher": "MichaelWatson",
  "icon": "images/workbench-icon.png",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "keywords": [
    "graphql",
    "embedding",
    "ai",
    "vector",
    "query generation",
    "workbench"
  ],
  "activationEvents": [
    "onLanguage:graphql",
    "workspaceContains:**/*.graphql",
    "workspaceContains:**/supergraph.yaml"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "graphql-workbench.embedFile",
        "title": "GraphQL Workbench: Embed Schema from File"
      },
      {
        "command": "graphql-workbench.embedEndpoint",
        "title": "GraphQL Workbench: Embed Schema from Endpoint"
      },
      {
        "command": "graphql-workbench.generateOperation",
        "title": "GraphQL Workbench: Generate Operation"
      },
      {
        "command": "graphql-workbench.clearEmbeddings",
        "title": "GraphQL Workbench: Clear All Embeddings"
      },
      {
        "command": "graphql-workbench.introspectEndpoint",
        "title": "GraphQL Workbench: Introspect Endpoint to File"
      },
      {
        "command": "graphql-workbench.lintSchema",
        "title": "GraphQL Workbench: Lint Schema"
      },
      {
        "command": "graphql-workbench.analyzeSchemaDesign",
        "title": "GraphQL Workbench: Analyze Schema Design"
      },
      {
        "command": "graphql-workbench.openExplorerPanel",
        "title": "GraphQL Workbench: Open Explorer Panel"
      },
      {
        "command": "graphql-workbench.dismissLintViolation",
        "title": "GraphQL Workbench: Dismiss Lint Violation"
      },
      {
        "command": "graphql-workbench.dismissAllLintViolations",
        "title": "GraphQL Workbench: Dismiss All Lint Violations"
      },
      {
        "command": "graphql-workbench.refreshDesigns",
        "title": "GraphQL Workbench: Refresh Designs",
        "icon": "$(refresh)"
      },
      {
        "command": "graphql-workbench.createDesign",
        "title": "GraphQL Workbench: New Schema Design",
        "icon": "$(add)"
      },
      {
        "command": "graphql-workbench.createFederatedDesign",
        "title": "GraphQL Workbench: New Federated Design"
      },
      {
        "command": "graphql-workbench.addSubgraph",
        "title": "GraphQL Workbench: Add Subgraph",
        "icon": "$(add)"
      },
      {
        "command": "graphql-workbench.openSchema",
        "title": "GraphQL Workbench: Open Schema"
      },
      {
        "command": "graphql-workbench.validateDesign",
        "title": "GraphQL Workbench: Validate Design",
        "icon": "$(check)"
      },
      {
        "command": "graphql-workbench.deleteDesign",
        "title": "GraphQL Workbench: Delete Design",
        "icon": "$(trash)"
      },
      {
        "command": "graphql-workbench.analyzeSchemaFromTree",
        "title": "Analyze Schema Design"
      },
      {
        "command": "graphql-workbench.lintSchemaFromTree",
        "title": "Lint Schema"
      },
      {
        "command": "graphql-workbench.renameSubgraph",
        "title": "Rename Subgraph"
      },
      {
        "command": "graphql-workbench.deleteSubgraph",
        "title": "Delete Subgraph",
        "icon": "$(trash)"
      },
      {
        "command": "graphql-workbench.embedDesign",
        "title": "Embed Schema",
        "icon": "$(database)"
      },
      {
        "command": "graphql-workbench.generateOperationForDesign",
        "title": "Generate Operation"
      },
      {
        "command": "graphql-workbench.clearDesignEmbeddings",
        "title": "Clear Embeddings"
      },
      {
        "command": "graphql-workbench.reEmbedDesign",
        "title": "Re-embed Schema"
      },
      {
        "command": "graphql-workbench.embeddingStatusClick",
        "title": "Embed Schema"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "schema-design-workbench",
          "title": "Schema Design Workbench",
          "icon": "images/workbench-icon.svg"
        }
      ]
    },
    "views": {
      "schema-design-workbench": [
        {
          "id": "schema-design-workbench.designs",
          "name": "Designs",
          "when": "config.graphqlWorkbench.enableDesignWorkbench"
        }
      ]
    },
    "configuration": {
      "title": "GraphQL Workbench",
      "properties": {
        "graphqlWorkbench.vectorStore": {
          "type": "string",
          "enum": [
            "pglite",
            "postgres"
          ],
          "default": "pglite",
          "description": "Vector store to use for embeddings"
        },
        "graphqlWorkbench.postgresConnectionString": {
          "type": "string",
          "default": "postgresql://postgres@localhost:5432/postgres",
          "description": "PostgreSQL connection string (when using postgres vector store)"
        },
        "graphqlWorkbench.modelPath": {
          "type": "string",
          "default": "",
          "description": "Path to custom GGUF embedding model (leave empty to auto-download the default model on first use)"
        },
        "graphqlWorkbench.llmProvider": {
          "type": "string",
          "enum": [
            "ollama",
            "ollama-cloud",
            "openai",
            "anthropic"
          ],
          "default": "ollama",
          "description": "LLM provider for dynamic operation generation"
        },
        "graphqlWorkbench.llmModel": {
          "type": "string",
          "default": "",
          "description": "LLM model to use (leave empty for provider default: qwen2.5 for Ollama/Ollama Cloud, gpt-4o-mini for OpenAI, claude-3-haiku for Anthropic)"
        },
        "graphqlWorkbench.ollamaBaseUrl": {
          "type": "string",
          "default": "http://localhost:11434",
          "description": "Ollama API base URL"
        },
        "graphqlWorkbench.ollamaCloudApiKey": {
          "type": "string",
          "default": "",
          "description": "Ollama Cloud API key (required when using ollama-cloud provider)"
        },
        "graphqlWorkbench.openaiApiKey": {
          "type": "string",
          "default": "",
          "description": "OpenAI API key (required when using OpenAI provider)"
        },
        "graphqlWorkbench.anthropicApiKey": {
          "type": "string",
          "default": "",
          "description": "Anthropic API key (required when using Anthropic provider)"
        },
        "graphqlWorkbench.llmTemperature": {
          "type": "number",
          "default": 0.2,
          "minimum": 0,
          "maximum": 2,
          "description": "LLM temperature for operation generation (lower = more deterministic)"
        },
        "graphqlWorkbench.llmTopK": {
          "type": "number",
          "default": 40,
          "minimum": 1,
          "maximum": 100,
          "description": "Top-K sampling parameter for LLM (limits token choices to top K most likely)"
        },
        "graphqlWorkbench.llmTopP": {
          "type": "number",
          "default": 0.9,
          "minimum": 0,
          "maximum": 1,
          "description": "Top-P (nucleus) sampling parameter for LLM (cumulative probability threshold)"
        },
        "graphqlWorkbench.minSimilarityScore": {
          "type": "number",
          "default": 0.4,
          "minimum": 0,
          "maximum": 1,
          "description": "Minimum cosine similarity score for vector search results (0-1)"
        },
        "graphqlWorkbench.maxDocuments": {
          "type": "number",
          "default": 50,
          "minimum": 1,
          "maximum": 200,
          "description": "Maximum number of documents to retrieve from vector search"
        },
        "graphqlWorkbench.maxValidationRetries": {
          "type": "number",
          "default": 5,
          "minimum": 1,
          "maximum": 10,
          "description": "Maximum attempts to fix invalid generated operations"
        },
        "graphqlWorkbench.enableDesignWorkbench": {
          "type": "boolean",
          "default": true,
          "description": "Show the Schema Design Workbench activity bar tab"
        },
        "graphqlWorkbench.roverPath": {
          "type": "string",
          "default": "rover",
          "description": "Path to the Rover CLI executable for federated schema validation"
        },
        "graphqlWorkbench.validateOnSave": {
          "type": "boolean",
          "default": true,
          "description": "Automatically validate schema designs on save"
        }
      }
    },
    "menus": {
      "commandPalette": [
        {
          "command": "graphql-workbench.dismissLintViolation",
          "when": "false"
        },
        {
          "command": "graphql-workbench.dismissAllLintViolations",
          "when": "false"
        },
        {
          "command": "graphql-workbench.addSubgraph",
          "when": "false"
        },
        {
          "command": "graphql-workbench.openSchema",
          "when": "false"
        },
        {
          "command": "graphql-workbench.deleteDesign",
          "when": "false"
        },
        {
          "command": "graphql-workbench.analyzeSchemaFromTree",
          "when": "false"
        },
        {
          "command": "graphql-workbench.lintSchemaFromTree",
          "when": "false"
        },
        {
          "command": "graphql-workbench.renameSubgraph",
          "when": "false"
        },
        {
          "command": "graphql-workbench.deleteSubgraph",
          "when": "false"
        },
        {
          "command": "graphql-workbench.embedDesign",
          "when": "false"
        },
        {
          "command": "graphql-workbench.generateOperationForDesign",
          "when": "false"
        },
        {
          "command": "graphql-workbench.clearDesignEmbeddings",
          "when": "false"
        },
        {
          "command": "graphql-workbench.reEmbedDesign",
          "when": "false"
        },
        {
          "command": "graphql-workbench.embeddingStatusClick",
          "when": "false"
        }
      ],
      "explorer/context": [
        {
          "command": "graphql-workbench.embedFile",
          "when": "resourceExtname == .graphql",
          "group": "graphql-workbench"
        },
        {
          "command": "graphql-workbench.lintSchema",
          "when": "resourceExtname == .graphql",
          "group": "graphql-workbench"
        }
      ],
      "editor/context": [
        {
          "command": "graphql-workbench.embedFile",
          "when": "resourceExtname == .graphql",
          "group": "graphql-workbench"
        },
        {
          "command": "graphql-workbench.generateOperation",
          "when": "resourceExtname == .graphql",
          "group": "graphql-workbench"
        },
        {
          "command": "graphql-workbench.lintSchema",
          "when": "resourceExtname == .graphql",
          "group": "graphql-workbench"
        },
        {
          "command": "graphql-workbench.openExplorerPanel",
          "when": "resourceExtname == .graphql",
          "group": "graphql-workbench"
        }
      ],
      "view/title": [
        {
          "command": "graphql-workbench.createDesign",
          "when": "view == schema-design-workbench.designs",
          "group": "navigation@1"
        },
        {
          "command": "graphql-workbench.refreshDesigns",
          "when": "view == schema-design-workbench.designs",
          "group": "navigation@2"
        },
        {
          "command": "graphql-workbench.createFederatedDesign",
          "when": "view == schema-design-workbench.designs"
        }
      ],
      "view/item/context": [
        {
          "command": "graphql-workbench.validateDesign",
          "when": "view == schema-design-workbench.designs && viewItem =~ /^design-/",
          "group": "inline"
        },
        {
          "command": "graphql-workbench.addSubgraph",
          "when": "view == schema-design-workbench.designs && viewItem == design-federated",
          "group": "inline"
        },
        {
          "command": "graphql-workbench.openSchema",
          "when": "view == schema-design-workbench.designs && viewItem == schema-file",
          "group": "0_navigation"
        },
        {
          "command": "graphql-workbench.analyzeSchemaFromTree",
          "when": "view == schema-design-workbench.designs && viewItem == schema-file",
          "group": "1_actions@1"
        },
        {
          "command": "graphql-workbench.lintSchemaFromTree",
          "when": "view == schema-design-workbench.designs && viewItem == schema-file",
          "group": "1_actions@2"
        },
        {
          "command": "graphql-workbench.analyzeSchemaFromTree",
          "when": "view == schema-design-workbench.designs && viewItem == design-standalone",
          "group": "1_actions@1"
        },
        {
          "command": "graphql-workbench.lintSchemaFromTree",
          "when": "view == schema-design-workbench.designs && viewItem == design-standalone",
          "group": "1_actions@2"
        },
        {
          "command": "graphql-workbench.renameSubgraph",
          "when": "view == schema-design-workbench.designs && viewItem == subgraph",
          "group": "1_actions"
        },
        {
          "command": "graphql-workbench.embedDesign",
          "when": "view == schema-design-workbench.designs && viewItem =~ /^design-/",
          "group": "2_embedding@1"
        },
        {
          "command": "graphql-workbench.generateOperationForDesign",
          "when": "view == schema-design-workbench.designs && viewItem =~ /^design-/",
          "group": "2_embedding@2"
        },
        {
          "command": "graphql-workbench.embedDesign",
          "when": "view == schema-design-workbench.designs && viewItem == embedding-status",
          "group": "1_embedding@1"
        },
        {
          "command": "graphql-workbench.reEmbedDesign",
          "when": "view == schema-design-workbench.designs && viewItem == embedding-status",
          "group": "1_embedding@2"
        },
        {
          "command": "graphql-workbench.clearDesignEmbeddings",
          "when": "view == schema-design-workbench.designs && viewItem =~ /^design-/",
          "group": "3_danger@1"
        },
        {
          "command": "graphql-workbench.deleteDesign",
          "when": "view == schema-design-workbench.designs && viewItem =~ /^design-/",
          "group": "3_danger@2"
        },
        {
          "command": "graphql-workbench.clearDesignEmbeddings",
          "when": "view == schema-design-workbench.designs && viewItem == embedding-status",
          "group": "2_danger"
        },
        {
          "command": "graphql-workbench.deleteSubgraph",
          "when": "view == schema-design-workbench.designs && viewItem == subgraph",
          "group": "2_danger"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "node esbuild.config.mjs",
    "watch": "node esbuild.config.mjs --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "package": "npm run build && node scripts/prepare-package.mjs",
    "publish": "vsce publish",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.2.0",
    "node-llama-cpp": "^3.0.0",
    "yaml": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/pg": "^8.11.0",
    "@types/vscode": "^1.85.0",
    "@vscode/vsce": "^3.2.1",
    "esbuild": "^0.20.0",
    "graphql": "^16.8.1",
    "graphql-embedding": "^0.1.0",
    "graphql-embedding-core": "^0.1.0",
    "graphql-embedding-operation": "^0.1.0",
    "graphql-embedding-parser": "^0.1.0",
    "graphql-embedding-schema-design": "^0.1.0",
    "pg": "^8.11.3",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT"
}
